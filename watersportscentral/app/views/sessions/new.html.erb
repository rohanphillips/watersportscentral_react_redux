<h1>Login</h1>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %><br>
  <br><p>or</p><br>

  <%= form_tag '/login' do %>
    <% flash.each do |type, msg| %>
      <div>
        <%= msg %>
      </div>
    <% end %>
    <%= label_tag :username%>
    <%= text_field_tag :username %>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
    <%= submit_tag "Login"%>
  <%end%>